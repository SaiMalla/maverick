<p id="notice"><%= notice %></p>

<h1>Ticket Sub Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Ticketsubstatus</th>
      <th>Creatredby</th>
      <th>Ticket status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ticket_sub_statuses.each do |ticket_sub_status| %>
      <tr>
        <td><%= ticket_sub_status.ticketsubstatus %></td>
        <td><%= ticket_sub_status.creatredby %></td>
        <td><%= ticket_sub_status.ticket_status %></td>
        <td><%= link_to 'Show', ticket_sub_status %></td>
        <td><%= link_to 'Edit', edit_ticket_sub_status_path(ticket_sub_status) %></td>
        <td><%= link_to 'Destroy', ticket_sub_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket Sub Status', new_ticket_sub_status_path %>
