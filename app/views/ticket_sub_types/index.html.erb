<p id="notice"><%= notice %></p>

<h1>Ticket Sub Types</h1>

<table>
  <thead>
    <tr>
      <th>Ticketsubtype</th>
      <th>Createdby</th>
      <th>Active</th>
      <th>Ticket type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ticket_sub_types.each do |ticket_sub_type| %>
      <tr>
        <td><%= ticket_sub_type.ticketsubtype %></td>
        <td><%= ticket_sub_type.createdby %></td>
        <td><%= ticket_sub_type.active %></td>
        <td><%= ticket_sub_type.ticket_type %></td>
        <td><%= link_to 'Show', ticket_sub_type %></td>
        <td><%= link_to 'Edit', edit_ticket_sub_type_path(ticket_sub_type) %></td>
        <td><%= link_to 'Destroy', ticket_sub_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket Sub Type', new_ticket_sub_type_path %>
